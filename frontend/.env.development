# Development Environment Variables
# For network access with specific IP

# Enable HTTPS (set to false for HTTP-only development)
VITE_USE_HTTPS=false

# Port for Vite server (5173 for network access)
VITE_PORT=5173

# Backend API URL (specific IP for network access)
VITE_BACKEND_URL=http://192.168.0.98:8000

# API base URL (empty to use proxy through same origin)
VITE_API_URL=
