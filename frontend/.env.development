# Development Environment Variables
# For local development with HTTPS

# Enable HTTPS (set to false for HTTP-only development)
VITE_USE_HTTPS=false

# Port for Vite server (5173 = development, 443 = production-like)
VITE_PORT=5173

# Backend API URL
VITE_BACKEND_URL=http://dropboxaiorganizer.com:8000

# API base URL (empty to use proxy through same origin)
VITE_API_URL=
