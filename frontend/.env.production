# Production Environment Variables
# For deployment with HTTPS on port 443

# Enable HTTPS (default: true)
VITE_USE_HTTPS=true

# Port for Vite server (443 = standard HTTPS, no port shown in URL)
VITE_PORT=443

# Backend API URL
VITE_BACKEND_URL=http://localhost:8000

# API base URL (empty to use proxy through same origin)
VITE_API_URL=
